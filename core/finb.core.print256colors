#!/bin/bash

echo
for finb_core_print256colors_fgbg in 38 48
do
    for finb_core_print256colors_color in {0..255}
    do
        printf "\e[${finb_core_print256colors_fgbg};5;%sm  %3s  \e[0m" $finb_core_print256colors_color $finb_core_print256colors_color
        if [[ $((($finb_core_print256colors_color + 1) % 6)) == 4 ]]
        then
            echo
        fi
    done
    echo
done